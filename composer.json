{
    "name": "dedoc/documentor",
    "description": "This is my package documentor",
    "keywords": [
        "dedoc",
        "laravel",
        "documentor"
    ],
    "homepage": "https://github.com/dedoc/documentor",
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Lytvynenko",
            "email": "litvinenko95@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/contracts": "^8.0.0|^9.0.0",
        "nikic/php-parser": "^4.14",
        "phpstan/phpdoc-parser": "^1.6",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "laravel/pint": "^v1.1.0",
        "nunomaduro/collision": "^5.0",
        "orchestra/testbench": "^6.0|^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/pest-plugin-snapshots": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Dedoc\\Documentor\\": "src",
            "Dedoc\\Documentor\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dedoc\\Documentor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dedoc\\Documentor\\DocumentorServiceProvider"
            ],
            "aliases": {
                "Documentor": "Dedoc\\Documentor\\Facades\\Documentor"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
